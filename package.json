  "scripts": {
    "dev": "node -e \"const h=require('http');const f=require('fs');const p=require('path');h.createServer((q,r)=>{let u=q.url==='/'?'/index.html':q.url;let fp=p.join(__dirname,'public',u);f.existsSync(fp)?r.end(f.readFileSync(fp)):r.writeHead(404).end('Not found')}).listen(3000,()=>console.log('http://localhost:3000'))\"",
    "build": "echo 'No build needed'"
  },
  "keywords": [
    "psychology",
    "transcription",
    "azure",
    "speech-to-text"
  ],
